<?xml version="1.0" encoding="utf-8"?>
<Containers>
  <Date>2017-10-09</Date>
  <Beta>False</Beta>
  <Category>MediaServer:Video</Category>
  <Name>Channels DVR</Name>
  <Project>https://getchannels.com/dvr</Project>
  <Changes></Changes>
  <Overview>
    Channels DVR
  </Overview>
  <Description>
    The Channels DVR backend engine.[br][br]
    [b][span style='color: #E80000;']Directions:[/span][/b][br]
    [b]/data[/b] : This is where the DVR will store its databases and recordings.[br][br]
  </Description>
  <Registry>https://registry.hub.docker.com/u/fancybits/channels-dvr/</Registry>
  <GitHub>https://github.com/fancybits/unraid</GitHub>
  <Repository>fancybits/channels-dvr</Repository>
  <Support>https://community.getchannels.com</Support>
  <BindTime>true</BindTime>
  <Privileged>true</Privileged>
  <Networking>
    <Mode>host</Mode>
    <Publish/>
  </Networking>
  <Environment/>
  <Data>
    <Volume>
      <HostDir></HostDir>
      <ContainerDir>/data</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
    <Volume>
      <HostDir>/mnt/user/appdata/ChannelsDVR</HostDir>
      <ContainerDir>/channels-dvr</ContainerDir>
      <Mode>rw</Mode>
    </Volume>
  </Data>
  <WebUI>http://[IP]:[PORT:8089]/web/index.html</WebUI>
  <Banner></Banner>
  <Icon>https://getchannels.com/assets/img/avatar-120.png</Icon>
</Containers>
